<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity"
  minHeight="-Infinity"
  minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0"
  xmlns="http://javafx.com/javafx/8.0.121" fx:controller="com.rcv.GuiConfigController">
  <center>
    <TabPane BorderPane.alignment="CENTER" prefHeight="200.0" prefWidth="200.0"
      tabClosingPolicy="UNAVAILABLE">
      <Tab text="Untitled Tab 1">
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0"/>
      </Tab>
      <Tab text="Untitled Tab 2">
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0"/>
      </Tab>
      <Tab text="Untitled Tab 3">
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0"/>
      </Tab>
      <Tab text="Untitled Tab 4">
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0"/>
      </Tab>
    </TabPane>
  </center>
  <bottom>
    <ButtonBar BorderPane.alignment="CENTER" prefHeight="40.0" prefWidth="200.0">
      <buttons>
        <Button mnemonicParsing="false" onAction="#buttonMenuClicked" text="Back to Menu"/>
        <Button mnemonicParsing="false" text="Save Config"/>
      </buttons>
      <BorderPane.margin>
        <Insets/>
      </BorderPane.margin>
      <padding>
        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
      </padding>
    </ButtonBar>
  </bottom>
</BorderPane>
