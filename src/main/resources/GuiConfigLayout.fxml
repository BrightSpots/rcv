<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity"
  minHeight="-Infinity"
  minWidth="-Infinity" prefHeight="550.0" prefWidth="900.0"
  xmlns="http://javafx.com/javafx/8.0.121" fx:controller="com.rcv.GuiConfigController">
  <right>
    <TextArea BorderPane.alignment="TOP_LEFT" editable="false" minWidth="300.0" prefWidth="300.0"
      wrapText="true" fx:id="textAreaHelp"/>
  </right>
  <center>
    <VBox BorderPane.alignment="CENTER">
      <TabPane minHeight="450.0" tabClosingPolicy="UNAVAILABLE">
        <Tab text="Contest">
          <VBox spacing="8.0">
            <Label text="Contest Name *"/>
            <TextField maxWidth="220.0" promptText="Contest Name *" fx:id="textFieldContestName"/>
            <Label text="Output Directory *"/>
            <HBox alignment="CENTER_LEFT" spacing="8.0">
              <TextField prefWidth="400.0" promptText="Output Directory *"
                fx:id="textFieldOutputDirectory"/>
              <Button mnemonicParsing="false" onAction="#buttonOutputDirectoryClicked"
                text="Select"/>
            </HBox>
            <Label text="Contest Date"/>
            <HBox spacing="8.0">
              <DatePicker editable="false" prefWidth="150.0" fx:id="datePickerContestDate"/>
              <Button mnemonicParsing="false" onAction="#buttonClearDatePickerContestDateClicked"
                text="Clear"/>
            </HBox>
            <Label text="Contest Jurisdiction"/>
            <TextField maxWidth="220.0" promptText="Contest Jurisdiction"
              fx:id="textFieldContestJurisdiction"/>
            <Label text="Contest Office"/>
            <TextField maxWidth="220.0" promptText="Contest Office" fx:id="textFieldContestOffice"/>
            <Label text="Tabulate by Precinct"/>
            <HBox alignment="CENTER_LEFT" spacing="12.0">
              <RadioButton mnemonicParsing="false" text="True" fx:id="radioTabulateByPrecinctTrue">
                <toggleGroup>
                  <ToggleGroup fx:id="toggleTabulateByPrecinct"/>
                </toggleGroup>
              </RadioButton>
              <RadioButton mnemonicParsing="false" selected="true" text="False"
                toggleGroup="$toggleTabulateByPrecinct"/>
            </HBox>
            <padding>
              <Insets bottom="12.0" left="12.0" right="12.0" top="12.0"/>
            </padding>
          </VBox>
        </Tab>
        <Tab text="CVR Files">
          <VBox>
            <HBox alignment="CENTER_LEFT" spacing="4.0">
              <VBox spacing="4.0">
                <HBox alignment="CENTER_LEFT" spacing="4.0">
                  <TextField prefHeight="25.0" prefWidth="366.0" promptText="File Path *"
                    fx:id="textFieldCvrFilePath"/>
                  <Button mnemonicParsing="false" onAction="#buttonCvrFilePathClicked"
                    text="Select"/>
                </HBox>
                <HBox alignment="CENTER_LEFT" spacing="4.0">
                  <TextField prefHeight="25.0" prefWidth="160.0"
                    promptText="First Vote Column Index *"
                    fx:id="textFieldCvrFirstVoteCol"/>
                  <TextField prefHeight="25.0" prefWidth="110.0" promptText="ID Column Index"
                    fx:id="textFieldCvrIdCol"/>
                  <TextField prefHeight="25.0" prefWidth="140.0" promptText="Precinct Column Index"
                    fx:id="textFieldCvrPrecinctCol"/>
                </HBox>
                <TextField maxWidth="418.0" prefHeight="25.0" prefWidth="300.0"
                  promptText="Provider" fx:id="textFieldCvrProvider"/>
              </VBox>
              <VBox alignment="BOTTOM_LEFT" spacing="4.0">
                <padding>
                  <Insets bottom="4.0" left="4.0" right="4.0" top="4.0"/>
                </padding>
                <Button mnemonicParsing="false" onAction="#buttonAddCvrFileClicked" text="Add"/>
                <Button mnemonicParsing="false" onAction="#buttonDeleteCvrFileClicked"
                  text="Delete Selected"/>
              </VBox>
              <padding>
                <Insets bottom="4.0" left="4.0" right="4.0" top="4.0"/>
              </padding>
            </HBox>
            <TableView fx:id="tableViewCvrFiles">
              <columns>
                <TableColumn editable="false" minWidth="20.0" prefWidth="250.0"
                  text="File Path" fx:id="tableColumnCvrFilePath"/>
                <TableColumn editable="false" minWidth="20.0" prefWidth="60.0"
                  text="First Vote&#10;Column&#10;Index" fx:id="tableColumnCvrFirstVoteCol"/>
                <TableColumn editable="false" minWidth="20.0" prefWidth="60.0"
                  text="ID&#10;Column&#10;Index" fx:id="tableColumnCvrIdCol"/>
                <TableColumn editable="false" minWidth="20.0" prefWidth="60.0"
                  text="Precinct&#10;Column&#10;Index" fx:id="tableColumnCvrPrecinctCol"/>
                <TableColumn editable="false" minWidth="20.0" prefWidth="160.0"
                  text="Provider" fx:id="tableColumnCvrProvider"/>
              </columns>
            </TableView>
            <padding>
              <Insets top="12.0"/>
            </padding>
          </VBox>
        </Tab>
        <Tab text="Candidates">
          <VBox>
            <HBox alignment="CENTER_LEFT" spacing="4.0">
              <TextField promptText="Name *" fx:id="textFieldCandidateName"/>
              <TextField promptText="Code" fx:id="textFieldCandidateCode"/>
              <Button mnemonicParsing="false" onAction="#buttonAddCandidateClicked" text="Add"/>
              <Region prefHeight="25.0" prefWidth="39.0"/>
              <Button mnemonicParsing="false" onAction="#buttonDeleteCandidateClicked"
                text="Delete Selected"/>
              <padding>
                <Insets bottom="4.0" left="4.0" right="4.0" top="4.0"/>
              </padding>
            </HBox>
            <TableView fx:id="tableViewCandidates">
              <columns>
                <TableColumn editable="false" minWidth="20.0" prefWidth="180.0"
                  text="Name" fx:id="tableColumnCandidateName"/>
                <TableColumn editable="false" minWidth="20.0" prefWidth="180.0"
                  text="Code" fx:id="tableColumnCandidateCode"/>
              </columns>
            </TableView>
            <padding>
              <Insets top="12.0"/>
            </padding>
          </VBox>
        </Tab>
        <Tab text="Rules">
          <HBox>
            <VBox spacing="8.0">
              <Label text="Tiebreak Mode *"/>
              <ChoiceBox prefWidth="220.0" fx:id="choiceTiebreakMode"/>
              <Label text="Max Rankings Allowed *"/>
              <TextField maxWidth="220.0" promptText="Max Rankings Allowed *"
                fx:id="textFieldMaxRankingsAllowed"/>
              <Label text="Max Skipped Ranks Allowed *"/>
              <TextField maxWidth="220.0" promptText="Max Skipped Ranks Allowed *"
                fx:id="textFieldMaxSkippedRanksAllowed"/>
              <Label text="Decimal Places for Vote Arithmetic"/>
              <TextField maxWidth="220.0" promptText="Decimal Places for Vote Arithmetic"
                fx:id="textFieldDecimalPlacesForVoteArithmetic"/>
              <Label text="Batch Elimination"/>
              <HBox alignment="CENTER_LEFT" spacing="12.0">
                <RadioButton mnemonicParsing="false" text="True" fx:id="radioBatchEliminationTrue">
                  <toggleGroup>
                    <ToggleGroup fx:id="toggleBatchElimination"/>
                  </toggleGroup>
                </RadioButton>
                <RadioButton mnemonicParsing="false" selected="true" text="False"
                  toggleGroup="$toggleBatchElimination"/>
              </HBox>
              <Label text="Exhaust on Duplicate Candidate"/>
              <HBox alignment="CENTER_LEFT" spacing="12.0">
                <RadioButton mnemonicParsing="false" text="True"
                  fx:id="radioExhaustOnDuplicateCandidateTrue">
                  <toggleGroup>
                    <ToggleGroup fx:id="toggleExhaustOnDuplicateCandidate"/>
                  </toggleGroup>
                </RadioButton>
                <RadioButton mnemonicParsing="false" selected="true" text="False"
                  toggleGroup="$toggleExhaustOnDuplicateCandidate"/>
              </HBox>
              <Label text="Overvote Label"/>
              <TextField maxWidth="220.0" promptText="Overvote Label"
                fx:id="textFieldOvervoteLabel"/>
              <Label text="Undeclared Write-in Label"/>
              <TextField maxWidth="220.0" promptText="Undeclared Write-in Label"
                fx:id="textFieldUndeclaredWriteInLabel"/>
              <padding>
                <Insets bottom="12.0" left="12.0" right="12.0" top="12.0"/>
              </padding>
            </VBox>
            <VBox spacing="8.0">
              <Label text="Overvote Rule *"/>
              <ChoiceBox prefWidth="220.0" fx:id="choiceOvervoteRule"/>
              <Label text="Multi-Seat Transfer Rule"/>
              <ChoiceBox prefWidth="220.0" fx:id="choiceMultiSeatTransferRule"/>
              <Label text="Number of Winners"/>
              <TextField maxWidth="220.0" promptText="Number of Winners"
                fx:id="textFieldNumberOfWinners"/>
              <Label text="Minimum Vote Threshold"/>
              <TextField maxWidth="220.0" promptText="Minimum Vote Threshold"
                fx:id="textFieldMinimumVoteThreshold"/>
              <Label text="Continue until Two Candidates Remain"/>
              <HBox alignment="CENTER_LEFT" spacing="12.0">
                <RadioButton mnemonicParsing="false" text="True"
                  fx:id="radioContinueUntilTwoCandidatesRemainTrue">
                  <toggleGroup>
                    <ToggleGroup fx:id="toggleContinueUntilTwoCandidatesRemain"/>
                  </toggleGroup>
                </RadioButton>
                <RadioButton mnemonicParsing="false" selected="true" text="False"
                  toggleGroup="$toggleContinueUntilTwoCandidatesRemain"/>
              </HBox>
              <Label text="Treat Blank as Undeclared Write-in"/>
              <HBox alignment="CENTER_LEFT" spacing="12.0">
                <RadioButton mnemonicParsing="false" text="True"
                  fx:id="radioTreatBlankAsUndeclaredWriteInTrue">
                  <toggleGroup>
                    <ToggleGroup fx:id="toggleTreatBlankAsUndeclaredWriteIn"/>
                  </toggleGroup>
                </RadioButton>
                <RadioButton mnemonicParsing="false" selected="true" text="False"
                  toggleGroup="$toggleTreatBlankAsUndeclaredWriteIn"/>
              </HBox>
              <Label text="Undervote Label"/>
              <TextField maxWidth="220.0" promptText="Undervote Label"
                fx:id="textFieldUndervoteLabel"/>
              <Label text="Rules Description"/>
              <TextField maxWidth="220.0" promptText="Rules Description"
                fx:id="textFieldRulesDescription"/>
              <padding>
                <Insets bottom="12.0" left="12.0" right="12.0" top="12.0"/>
              </padding>
            </VBox>
          </HBox>
        </Tab>
      </TabPane>
      <Region/>
      <ButtonBar maxHeight="50.0" prefHeight="50.0" fx:id="buttonBar">
        <buttons>
          <Button mnemonicParsing="false" onAction="#buttonMenuClicked" text="Back to Menu"/>
          <Button mnemonicParsing="false" onAction="#buttonValidateClicked" text="Validate Config"/>
          <Button mnemonicParsing="false" onAction="#buttonSaveClicked" text="Save Config"/>
        </buttons>
        <padding>
          <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
        </padding>
      </ButtonBar>
    </VBox>
  </center>
</BorderPane>
