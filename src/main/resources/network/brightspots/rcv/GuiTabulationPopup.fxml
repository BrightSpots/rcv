<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<Pane prefHeight="443.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="network.brightspots.rcv.GuiTabulateController">
   <children>
      <VBox layoutX="14.0" layoutY="10.0" prefHeight="259.0" prefWidth="573.0" spacing="10.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Configuration" wrappingWidth="176.1533203125">
               <font>
                  <Font name="System Bold" size="19.0" />
               </font>
            </Text>
            <HBox prefHeight="45.0" prefWidth="567.0" spacing="30.0">
               <children>
                  <TextArea fx:id="filepath" editable="false" prefHeight="39.0" minWidth="200.0" prefWidth="500.0" style="-fx-background-color: red;" text="C:/filepath.json">
                     <HBox.margin>
                        <Insets left="15.0" />
                     </HBox.margin>
                  </TextArea>
                  <Button fx:id="saveButton" mnemonicParsing="false" onAction="#buttonSaveClicked" prefHeight="42.0" minWidth="70.0" text="Save*" />
                  <Button fx:id="tempSaveButton" mnemonicParsing="false" onAction="#buttonTempSaveClicked" prefHeight="42.0" minWidth="155.0" text="Save Temporary Config" />
               </children>
            </HBox>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Configuration Summary" wrappingWidth="225.1533203125">
               <font>
                  <Font name="System Bold" size="19.0" />
               </font>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Text>
            <Label fx:id="numberOfCandidates" prefHeight="24.0" prefWidth="349.0" text="Number of Candidates: X" wrapText="true">
               <font>
                  <Font size="16.0" />
               </font>
               <VBox.margin>
                  <Insets left="15.0" />
               </VBox.margin>
            </Label>
            <Label fx:id="numberOfCvrs" prefHeight="24.0" prefWidth="349.0" text="Number of CVRs: Y" wrapText="true">
               <font>
                  <Font size="16.0" />
               </font>
               <VBox.margin>
                  <Insets left="15.0" />
               </VBox.margin>
            </Label>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Auditing Information" wrappingWidth="225.1533203125">
               <font>
                  <Font name="System Bold" size="19.0" />
               </font>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Text>
            <HBox prefHeight="100.0" prefWidth="200.0" spacing="30.0">
               <children>
                  <Label prefHeight="30.0" prefWidth="126.0" text="Enter your name:">
                     <font>
                        <Font size="15.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="15.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="userNameField" onKeyTyped="#nameUpdated" prefHeight="26.0" prefWidth="411.0" style="-fx-border-color: red;">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </TextField>
               </children>
            </HBox>
         </children>
      </VBox>
      <Text fx:id="progressText" layoutX="25" layoutY="412.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Current Step: x/y" textAlignment="CENTER" wrappingWidth="550">
         <font>
            <Font size="15.0" />
         </font>
      </Text>
      <ProgressBar fx:id="progressBar" layoutX="19.0" layoutY="362.0" prefHeight="20.0" prefWidth="563.0" progress="0.0" />
      <Button fx:id="tabulateButton" defaultButton="true" disable="true" mnemonicParsing="false" onAction="#buttonTabulateClicked"
              layoutX="200.0" layoutY="300.0" prefHeight="42.0" prefWidth="200.0" text="Tabulate" />
   </children>
</Pane>
